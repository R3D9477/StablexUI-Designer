<?xml version="1.0" encoding="UTF-8"?>
<ViewStack
	left="0"
	top="0"
	w="100"
	h="100"
	trans="(function () { var trans = new $Fade(); trans.duration = 0.25; return trans; })()">
	<VBox padding="5">
		<Button
			text="'Next'"
			on-click="$TypeCast.castType($this.parent.parent, $ViewStack).show($this.parent.parent.getChildAt(1).name);"/>
	</VBox>
	<VBox padding="5">
		<Button
			text="'Back'"
			on-click="$TypeCast.castType($this.parent.parent, $ViewStack).show($this.parent.parent.getChildAt(0).name);"/>
	</VBox>
</ViewStack>
