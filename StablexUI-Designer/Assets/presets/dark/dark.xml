<?xml version="1.0" encoding="UTF-8"?>

<Defaults>
	<GuiElements>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0x272727"/>
	</GuiElements>
	<Bmp>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</Bmp>
	<Box>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</Box>
	<Button>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			skin:Paint-corners="[5]"/>
	</Button>
	<Checkbox>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/checkbox_unchecked.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/checkbox_checked.png']))"/>
	</Checkbox>
	<Clock>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			paddingLeft="5"
			paddingRight="5"
			paddingTop="2"
			paddingBottom="2"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			skin:Paint-corners="[10]"/>
	</Clock>
	<Floating>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0x272727"/>
	</Floating>
	<HBox>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</HBox>
	<InputText>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</InputText>
	<Options>
		<Default
			alignList="true"
			format-size="12"
			format-color="0x0AD49E"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			box-skin:Paint-alpha="0.9"
			list-w="$this.box.w"
			list-h="$this.box.h"
			list-top="$this.top + $this.h"
			list-left="$this.left"
			list-skin="null"
			optionDefaults="'Option'"/>
	</Options>
	<Progress>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			skin:Paint-corners="[10]"
			bar-skin:Gradient-colors="[0x486A61, 0x218E71]"
			bar-skin:Gradient-border="1"
			bar-skin:Gradient-borderColor="0x04D9EB"
			bar-skin:Gradient-corners="[10]"/>
	</Progress>
	<Radio>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/radio_unselected.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/radio_selected.png']))"/>
	</Radio>
	<Scroll>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			vBar-visible="true"
			vBar-w="10"
			vBar-skin:Paint-border="0"
			vBar-slider-widthPt="100"
			vBar-slider-skin:Paint-color="0x008491"
			vBar-slider-skin:Paint-border="0"
			hBar-visible="true"
			hBar-h="10"
			hBar-skin:Paint-border="0"
			hBar-slider-heightPt="100"
			hBar-slider-skin:Paint-color="0x008491"
			hBar-slider-skin:Paint-border="0"/>
	</Scroll>
	<Slider>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			skin:Paint-corners="[10]"
			slider-skin:Paint-color="0x008491"
			slider-skin:Paint-border="1"
			slider-skin:Paint-borderColor="0x04D9EB"
			slider-skin:Paint-corners="[10]"/>
	</Slider>
	<StateButton>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			states-one-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/statebutton_state1.png']))"
			states-two-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/statebutton_state2.png']))"
			states-three-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/statebutton_state3.png']))"/>
	</StateButton>
	<Switch>
		<Default
			skin:Paint-corners="[10]"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"
			slider-w="50"
			slider-heightPt="100"
			slider-skin:Gradient-border="1"
			slider-skin:Gradient-borderColor="0x04D9EB"
			slider-skin:Gradient-corners="[10]"
			slider-skin:Gradient-colors="[0x486A61, 0x218E71]"
			labelOn-format-size="12"
			labelOn-format-color="0x0AD49E"
			labelOff-format-size="12"
			labelOff-format-color="0x0AD49E"/>
	</Switch>
	<TabPage>
		<Default
			widthPt="100"
			heightPt="100"
			title-format-size="12"
			title-format-color="0x0AD49E"
			title-states-up-ico="null"
			title-states-down-ico="null"
			title-highlightFormat-color="0xEDD400"
			title-highlightFormat-bold="true"
			title-highlightOnSelect="true"/>
	</TabPage>
	<TabStack>
		<Default
			padding="5"
			autoSize="true"
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</TabStack>
	<Text>
		<Default
			format-size="12"
			format-color="0x0AD49E"/>
	</Text>
	<Tip>
		<Default/>
	</Tip>
	<Toggle>
		<Default
			format-size="12"
			format-color="0x0AD49E"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/toggle_off.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/dark/img/toggle_on.png']))"/>
		<Option
			format-size="12"
			format-color="0x0AD49E"/>
	</Toggle>
	<VBox>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</VBox>
	<ViewStack>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</ViewStack>
	<Widget>
		<Default
			skin:Paint-color="0x272727"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x04D9EB"/>
	</Widget>
</Defaults>
