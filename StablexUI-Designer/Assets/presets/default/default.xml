<?xml version="1.0" encoding="UTF-8"?>

<Defaults>
	<GuiElements>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</GuiElements>
	<Bmp>
		<Default
			w="50"
			h="50"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Bmp>
	<Box>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Box>
	<Button>
		<Default
			format-size="12"
			format-color="0x000000"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Button>
	<Checkbox>
		<Default
			format-size="12"
			format-color="0x000000"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/checkbox_unchecked.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/checkbox_checked.png']))"/>
	</Checkbox>
	<Clock>
		<Default
			format-size="12"
			format-color="0x000000"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Clock>
	<Floating>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Floating>
	<HBox>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</HBox>
	<InputText>
		<Default
			w="100"
			h="20"
			format-size="12"
			format-color="0x000000"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</InputText>
	<Options>
		<Default
			w="150"
			h="20"
			format-size="12"
			format-color="0x000000"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			box-skin:Paint-alpha="0.9"
			list-skin:Paint-color="0xC0C0C0"
			list-skin:Paint-alpha="1"/>
	</Options>
	<Progress>
		<Default
			w="200"
			h="20"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			bar-skin:Gradient-corners="[10]"
			bar-skin:Gradient-colors="[0xAAAAAA, 0xFFFFFF]"/>
	</Progress>
	<Radio>
		<Default
			format-size="12"
			format-color="0x000000"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/radio_unselected.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/radio_selected.png']))"/>
	</Radio>
	<Scroll>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			vBar-visible="true"
			vBar-skin:Paint-color="0xFFFFFF"
			vBar-skin:Paint-border="1"
			vBar-skin:Paint-borderColor="0x000000"
			vBar-slider-skin:Paint-color="0xAAAAAA"
			hBar-visible="true"
			hBar-skin:Paint-color="0xFFFFFF"
			hBar-skin:Paint-border="1"
			hBar-skin:Paint-borderColor="0x000000"
			hBar-slider-skin:Paint-color="0xAAAAAA"/>
	</Scroll>
	<Slider>
		<Default
			w="100"
			h="20"
			skin:Paint-corners="[10]"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			slider-w="50"
			slider-heightPt="100"
			slider-skin:Gradient-corners="[10]"
			slider-skin:Gradient-colors="[0xAAAAAA, 0xFFFFFF]"/>
	</Slider>
	<StateButton>
		<Default
			format-size="12"
			format-color="0x000000"
			states-one-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state1.png']))"
			states-two-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state2.png']))"
			states-three-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state3.png']))"/>
	</StateButton>
	<Switch>
		<Default
			w="100"
			h="20"
			skin:Paint-corners="[10]"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			slider-w="50"
			slider-heightPt="100"
			slider-skin:Gradient-corners="[10]"
			slider-skin:Gradient-colors="[0xAAAAAA, 0xFFFFFF]"/>
	</Switch>
	<TabPage>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="0"
			title-format-size="12"
			title-format-color="0x000000"
			title-states-up-ico="null"
			title-states-down-ico="null"/>
	</TabPage>
	<TabStack>
		<Default
			padding="5"
			autoSize="true"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			tabBar-paddingBottom="5"
			tabBar-skin:Paint-color="0xC0C0C0"
			tabBar-skin:Paint-border="0"/>
	</TabStack>
	<Text>
		<Default
			format-size="12"
			format-color="0x000000"/>
	</Text>
	<Tip>
		<Default/>
	</Tip>
	<Toggle>
		<Default
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/toggle_off.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/toggle_on.png']))"/>
	</Toggle>
	<VBox>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</VBox>
	<ViewStack>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</ViewStack>
	<Widget>
		<Default
			w="100"
			h="100"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Widget>
</Defaults>
