<?xml version="1.0" encoding="UTF-8"?>

<Defaults>
	<GuiElements>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xC0C0C0"/>
	</GuiElements>
	<Bmp>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Bmp>
	<Box>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Box>
	<Button>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			skinHovered:Paint-color="0xC0C0C0"
			skinHovered:Paint-border="1"
			skinHovered:Paint-borderColor="0xFF0000"
			skinPressed:Paint-color="0x9C9C9C"
			skinPressed:Paint-border="1"
			skinPressed:Paint-borderColor="0xFF0000"/>
		<ChildWindowClose
			text="'X'"/>
	</Button>
	<Checkbox>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/checkbox_unchecked.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/checkbox_checked.png']))"/>
	</Checkbox>
	<Clock>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			paddingLeft="5"
			paddingRight="5"
			paddingTop="2"
			paddingBottom="2"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</Clock>
	<Floating>
		<Default
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xC0C0C0"/>
	</Floating>
	<HBox>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
		<ChildWindowHeader
			widthPt="100"
			heightPt="100"
			layout:Column-cols="[-1, 20]"/>
	</HBox>
	<InputText>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</InputText>
	<Options>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			box-skin:Paint-alpha="0.5"
			box-skin:Paint-color="0x000000"
			box-skin:Paint-border="1"
			box-skin:Paint-borderColor="0x000000"
			optionDefaults="'Option'"/>
	</Options>
	<Progress>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			skin:Paint-corners="[10]"
			bar-skin:Gradient-colors="[0xAAAAAA, 0xFFFFFF]"
			bar-skin:Gradient-border="1"
			bar-skin:Gradient-borderColor="0x000000"
			bar-skin:Gradient-corners="[10]"/>
	</Progress>
	<Radio>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/radio_unselected.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/radio_selected.png']))"/>
	</Radio>
	<Scroll>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="0"
			vBar-visible="true"
			vBar-w="10"
			vBar-skin:Paint-border="0"
			vBar-skin:Paint-corners="null"
			vBar-slider-widthPt="100"
			vBar-slider-skin:Paint-color="0x909090"
			vBar-slider-skin:Paint-border="0"
			vBar-slider-skin:Paint-corners="null"
			hBar-visible="true"
			hBar-h="10"
			hBar-skin:Paint-border="0"
			hBar-skin:Paint-corners="null"
			hBar-slider-heightPt="100"
			hBar-slider-skin:Paint-color="0x909090"
			hBar-slider-skin:Paint-border="0"
			hBar-slider-skin:Paint-corners="null"/>
	</Scroll>
	<Slider>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"
			skin:Paint-corners="[10]"
			slider-skin:Paint-color="0x909090"
			slider-skin:Paint-border="1"
			slider-skin:Paint-corners="[10]"/>
	</Slider>
	<StateButton>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			states-one-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state1.png']))"
			states-two-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state2.png']))"
			states-three-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/statebutton_state3.png']))"/>
	</StateButton>
	<Switch>
		<Default
			skin:Paint-corners="[10]"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="2"
			skin:Paint-borderColor="0x000000"
			slider-w="50"
			slider-heightPt="100"
			slider-skin:Gradient-border="1"
			slider-skin:Gradient-borderColor="0x000000"
			slider-skin:Gradient-corners="[10]"
			slider-skin:Gradient-colors="[0xAAAAAA, 0xFFFFFF]"
			labelOn-format-size="13"
			labelOn-format-color="0x000000"
			labelOn-format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"
			labelOff-format-size="13"
			labelOff-format-color="0x000000"
			labelOff-format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"/>
	</Switch>
	<TabPage>
		<Default
			widthPt="100"
			heightPt="100"
			title-format-size="13"
			title-format-color="0x000000"
			title-highlightOnSelect="true"
			title-highlightFormat-color="0xFF0000"
			title-states-up-ico-skin="null"
			title-states-down-ico-skin="null"/>
	</TabPage>
	<TabStack>
		<Default
			tabBar-padding="5"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</TabStack>
	<Text>
		<Default
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"/>
	</Text>
	<Tip>
		<Default
			label-format-size="13"
			label-format-color="0x000000"
			label-format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"/>
	</Tip>
	<Toggle>
		<Default
			states-up-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/toggle_off.png']))"
			states-down-ico-skin:Img-bitmapData="$BitmapData.fromFile($Path.join([%CWD, 'presets/default/img/toggle_on.png']))"/>
		<Option
			format-size="13"
			format-color="0x000000"
			format-font="$Path.join([%CWD, 'fonts/DejaVuSans.ttf'])"/>
	</Toggle>
	<VBox>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
		<ChildWindow
			widthPt="100"
			heightPt="100"
			layout:Row-rows="[20, -1]"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</VBox>
	<ViewStack>
		<Default
			skin:Paint-color="0xFFFFFF"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
	</ViewStack>
	<Widget>
		<Default
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x000000"/>
		<ScrollWidget
			skin:Paint-color="0xFFFFFF"/>
		<ChildWindowHeader
			widthPt="100"
			heightPt="100"
			skin:Paint-color="0xFFFFFF"/>
	</Widget>
</Defaults>
