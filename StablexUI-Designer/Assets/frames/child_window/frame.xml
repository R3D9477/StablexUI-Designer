<?xml version="1.0" encoding="UTF-8"?>
<Floating
	w="$Suid.getSetting().guiWidth"
	h="$Suid.getSetting().guiHeight">
	<VBox
		defaults="'Default, ChildWindow'">
		<HBox
			defaults="'Default, ChildWindowHeader'">
			<Widget/>
			<Button
				defaults="'Default, ChildWindowClose'"
				on-click="$Suid.castType($this.parent.parent.parent, $Floating).hide();"/>
		</HBox>
		<GuiElements/>
	</VBox>
</Floating>
