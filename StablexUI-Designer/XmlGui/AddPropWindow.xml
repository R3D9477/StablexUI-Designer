<?xml version="1.0" encoding="UTF-8"?>
<Floating
	w="300"
	h="220"
	skin:Paint-color="0xC0C0C0"
	skin:Paint-border="1"
	skin:Paint-borderColor="0xFFFFFF">
	<VBox
		widthPt="100"
		heightPt="100"
		layout:Row-rows="[110, 50, 20]"
		layout:Row-paddingTop="10"
		layout:Row-cellPadding="10"
		layout:Row-paddingLeft="10"
		layout:Row-paddingRight="10">
		<VBox
			widthPt="100"
			layout:Row-rows="[0, 0, 0, 0]"
			layout:Row-paddingTop="5"
			layout:Row-cellPadding="5"
			layout:Row-paddingLeft="5"
			layout:Row-paddingRight="5"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0xFFFFFF">
			<Text text="'Builder:'"/>
			<HBox
				layout:Column-cols="[0, -1]"
				layout:Column-cellPadding="10">
				<Text text="'Type:'"/>
				<Options
					name="'wgtPropTypesLst'"
					box-skin:Paint-alpha="0.9"
					skin:Paint-color="0xC0C0C0"
					skin:Paint-border="1"
					skin:Paint-borderColor="0x909090"
					list-skin:Paint-color="0x454545"
					list-skin:Paint-alpha="1"/>
			</HBox>
			<HBox
				layout:Column-cols="[0, -1]"
				layout:Column-cellPadding="10">
				<Text text="'Property:'"/>
				<Options
					name="'wgtPropNamesLst'"
					box-skin:Paint-alpha="0.9"
					skin:Paint-color="0xC0C0C0"
					skin:Paint-border="1"
					skin:Paint-borderColor="0x909090"
					list-skin:Paint-color="0x454545"
					list-skin:Paint-alpha="1"/>
			</HBox>
			<HBox
				layout:Column-cols="[.5, .5]"
				layout:Column-cellPadding="10">
				<Button
					name="'wgtAddPropRefresh'"
					text="'Refresh'"
					skin:Paint-color="0xC0C0C0"
					skin:Paint-border="1"
					skin:Paint-borderColor="0x909090"/>
				<Button
					name="'wgtAddPropBack'"
					text="'Back'"
					skin:Paint-color="0xC0C0C0"
					skin:Paint-border="1"
					skin:Paint-borderColor="0x909090"/>
			</HBox>
		</VBox>
		<VBox
			widthPt="100"
			layout:Row-rows="[0, 0]"
			layout:Row-paddingTop="5"
			layout:Row-cellPadding="5"
			layout:Row-paddingLeft="5"
			layout:Row-paddingRight="5"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0xFFFFFF">
			<Text text="'Prototype:'"/>
			<InputText
				name="'wgtPropPrototype'"
				label-wordWrap="true"
				skin:Paint-color="0xC0C0C0"
				skin:Paint-border="1"
				skin:Paint-borderColor="0x909090"/>
		</VBox>
		<Button
			name="'wgtAddPropBtn'"
			text="'Add'"
			skin:Paint-color="0xC0C0C0"
			skin:Paint-border="1"
			skin:Paint-borderColor="0x909090"/>
	</VBox>
</Floating>
